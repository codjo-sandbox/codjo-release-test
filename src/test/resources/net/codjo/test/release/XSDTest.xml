        <extraArg line="-DendDate 20121227"/>
    </batch-export>

    <batch-segmentation initiator="seb" segmentations="1, 2" date="2010-01-01" returnCode="5">
        <extraArg line="-DbeginDate 19700101"/>
        <extraArg line="-DendDate 20121227"/>
