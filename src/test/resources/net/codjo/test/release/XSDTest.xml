    </listen-jms>

    <!-- Format dates : yyyy-mm-dd hh:mm:ss.fffffffff -->
    <security-model user="zsazsa" roles="chef" lastLogin="" lastLogout=""/>

    <tokio-assert refId="refDB" allTables="true"/>
